// New York City I, 1942 by Piet Mondrian
// Make the canvas
void setup(){
  size(888,888);
}

// Make the background old paper white
void draw(){
  background(#ffffff);
  strokeWeight(30);
  stroke(#EFF23D);
  line(50, 0, 50, 959);
  line(0, 100, 500, 100);
  line(160, 0, 160, 900);
  line(500, 0, 500, 900);
  line(0, 700, 700, 700);
  line(100, 100, 100, 100);
  line(0, 780, 780, 780);
  line(600, 0, 600, 888);
}
